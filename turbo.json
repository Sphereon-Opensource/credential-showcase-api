{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**"]
    },
    "credential-showcase-api-server#build": {
      "dependsOn": [
        "^build",
        "credential-showcase-openapi#build"
      ],
      "env": [
        "DB_URL"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
